generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./prisma.db.sqlite"
}

model TwitterPosts {
  id                String   @id
  text              String
  createdByUsername String
  createdByUserId   String
  fullThread        String
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}
